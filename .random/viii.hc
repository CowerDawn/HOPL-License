U0 BeerCounter() {
    I64 beerCount = 0;
    U8 choice;

    "Welcome to the HOPL Beer Counter!\n";

    while (TRUE) {
        "\nWould you like to buy the author a beer? (y/n): ";
        choice = GetChar();

        if (choice == 'y') {
            beerCount++;
            "Thank you for the beer! Total beers: %d\n", beerCount;
        } else if (choice == 'n') {
            "\nThank you for your support! Total beers: %d\n", beerCount;
            break;
        } else {
            "Invalid choice. Please enter 'y' or 'n'.\n";
        }
    }
}

